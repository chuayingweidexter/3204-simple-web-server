server {
        listen 80;
        listen [::]:80;
        root /usr/share/nginx/3204-simple-web-server;
        index index.html index.htm index.nginx-debian.html;
        server_name 3204-simple-web-server.com www.3204-simple-web-server.com;

        location / {
               try_files $uri $uri/ =404;
        }
}
